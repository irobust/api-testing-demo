pipeline{
    agent any

    stages{
        stage('Build API'){
            steps {
                bat 'echo "Build API....."'
            }
        }

        stage('Start API'){
            steps {
                bat 'echo "Start API....."'
            }
        }

        stage('Test API'){
            steps {
                bat 'mvn clean test'
            }
        }
    }
}